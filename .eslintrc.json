{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "react/jsx-one-expression-per-line": 0,
    "max-len": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "react/jsx-boolean-value": 0,
    "no-multiple-empty-lines": 0,
    "padded-blocks": 0,
    "react/jsx-filename-extension": 0,
    "no-use-before-define": 0,
    "object-curly-newline": 0
  },
  "parser": "babel-eslint"
}
